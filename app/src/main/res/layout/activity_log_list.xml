<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".LogList"
    android:background="@drawable/button_background_x_flame_cut">

    <ImageButton
        android:layout_width="@dimen/life_goal_icon"
        android:layout_height="@dimen/life_goal_icon"
        android:id="@+id/imageButton"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:contentDescription="@string/life_goal_1"
        android:layout_toLeftOf="@+id/imageButton2"
        android:layout_toStartOf="@+id/imageButton2" />

    <ImageButton
        android:layout_width="@dimen/life_goal_icon"
        android:layout_height="@dimen/life_goal_icon"
        android:id="@+id/imageButton2"
        android:contentDescription="@string/life_goal_1"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="@dimen/life_goal_icon"
        android:layout_height="@dimen/life_goal_icon"
        android:id="@+id/imageButton3"
        android:contentDescription="@string/life_goal_1"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_toEndOf="@+id/imageButton2"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <RelativeLayout
        android:layout_width="120dp"
        android:layout_height="90dp"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/imageButton3"
        android:layout_toStartOf="@+id/imageButton3"
        android:id="@+id/relativeLayout">

        <ImageButton
            android:layout_width="@dimen/log_task_button"
            android:layout_height="@dimen/log_task_button"
            android:id="@+id/imageButton5"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:contentDescription="@string/log_task_button2"
            android:background="@drawable/log_task_button_2_cut" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_toStartOf="@+id/imageButton2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_above="@+id/relativeLayout"
        android:layout_toLeftOf="@+id/imageButton2"
        android:id="@+id/relativeLayout2"
        android:layout_below="@+id/imageButton">

        <ImageButton
            android:layout_width="@dimen/log_task_button"
            android:layout_height="@dimen/log_task_button"
            android:id="@+id/imageButton4"
            android:contentDescription="@string/log_task_button2"
            android:visibility="visible"
            android:background="@drawable/log_task_button_2_cut"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignBottom="@+id/relativeLayout2"
        android:layout_toRightOf="@+id/relativeLayout"
        android:layout_toEndOf="@+id/relativeLayout"
        android:layout_below="@+id/imageButton3">

        <ImageButton
            android:layout_width="@dimen/log_task_button"
            android:layout_height="@dimen/log_task_button"
            android:id="@+id/imageButton6"
            android:contentDescription="@string/log_task_button3"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:adjustViewBounds="false"
            android:background="@drawable/log_task_button_2_cut" />
    </RelativeLayout>

</RelativeLayout>
